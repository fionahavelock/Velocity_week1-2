1) SELECT * from [sakila].[actor];
2) SELECT * FROM [sakila].[actor] WHERE first_name='John';
3) SELECT * FROM [sakila].[actor] WHERE last_name='Neeson';
4) SELECT * FROM [sakila].[actor] WHERE (actor_id % 10)=0;
5) SELECT description FROM [sakila].[film] WHERE film_id=100;
6) SELECT * FROM [sakila].[film] WHERE rating='R';
7) SELECT * FROM [sakila].[film] WHERE rating!='R';
8) SELECT * FROM sakila.film WHERE special_features LIKE '%Deleted Scenes%';
9) SELECT last_name FROM sakila.actor GROUP BY last_name HAVING COUNT(actor_id)=1;
10) SELECT last_name FROM sakila.actor GROUP BY last_name HAVING COUNT(actor_id)>1;
11) SELECT a.actor_id, COUNT(f.actor_id) FROM sakila.actor a JOIN sakila.film_actor f ON a.actor_id=f.actor_id GROUP BY a.actor_id ORDER BY COUNT(f.actor_id) DESC;
12) SELECT * FROM sakila.inventory i JOIN sakila.film f ON i.film_id=f.film_id WHERE i.store_id=1 AND f.title='Academy Dinosaur';
13) SELECT AVG(length) FROM sakila.film;
14) SELECT AVG(length) FROM sakila.film f JOIN sakila.film_category fc ON f.film_id=fc.film_id GROUP BY fc.category_id;
15) SELECT title FROM sakila.film WHERE description LIKE '%Robot%';
16) SELECT length, title FROM sakila.film WHERE length=(SELECT MAX(length) FROM sakila.film);
17) SELECT COUNT(film_id) FROM sakila.film WHERE release_year=2010;
18) SELECT CONCAT(first_name, ' ', last_name) AS full_name FROM sakila.staff WHERE staff_id=1;
19) SELECT fa.actor_id, f.title FROM sakila.film_actor fa JOIN sakila.film f ON fa.film_id=f.film_id WHERE actor_id=(SELECT actor_id FROM sakila.actor WHERE first_name='Fred' AND last_name='Costner');
20) SELECT COUNT(film_id), store_id FROM sakila.inventory WHERE film_id=(SELECT film_id FROM sakila.film WHERE title='Bucket Brotherhood') GROUP BY store_id;
21) SELECT DISTINCT COUNT(country) FROM sakila.country
22) SELECT name FROM sakila.language ORDER BY name;
23) SELECT first_name, last_name FROM sakila.actor WHERE last_name LIKE '%son%' ORDER BY last_name;